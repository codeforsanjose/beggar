<header class="page-header">
  <h1>PRs</h1>
  <%= link_to "Getting Started?", page_path("setup") %>
</header>

<section class="slice">
  <div class="container">
    <ol class="tags">
      <li>
        <span class="slice-title">
          Filter:
        </span>
      </li>
      <%= render tags %>
    </ol>
  </div>
</section>

<main>
  <div class="container">
    <ol class="table-list">
      <%= render pull_requests %>
    </ol>
  </div>
</main>
